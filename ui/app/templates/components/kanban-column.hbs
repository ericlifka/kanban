<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">{{name}}</h3>
    </div>
    <div class="panel-body">
        {{#each card in cards}}
            <div class="card panel panel-info">
                <div class="panel-heading">
                    {{card.name}}
                </div>
            </div>
        {{/each}}
    </div>
</div>